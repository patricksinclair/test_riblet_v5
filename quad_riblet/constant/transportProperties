FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (water air);

water
{
    //represents ketchup here
    transportModel  Newtonian;
    nu              1e-3;
    rho             1000;
}

air
{
    //now represents water
    transportModel  Newtonian;
    nu              1e-06;
    rho             1000;
}

sigma            0.01;

